{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "allowJs": true,
    "checkJs": false,
    "strict": true,
    "alwaysStrict": true,
    "noImplicitAny": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "pretty": true,
    "strictNullChecks": true,
    "sourceMap": false,
    "noEmit": true
  },
  "include": [
    "./src"
  ],
  "exclude": [
    "node_modules"
  ]
}